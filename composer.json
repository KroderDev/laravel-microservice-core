{
    "name": "kroderdev/laravel-microservice-core",
    "description": "A toolkit to use Laravel effectively as a microservice in a distributed architecture.",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench": "^10.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kroderdev\\LaravelMicroserviceCore\\": "src/"
        }
    },
    "authors": [
        {
            "name": "KroderDev",
            "email": "142845401+KroderDev@users.noreply.github.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Kroderdev\\LaravelMicroserviceCore\\Providers\\MicroserviceServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
